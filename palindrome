#include <stdio.h>

int main()
{
    int i,rev=0,j,k;
    printf("enter an integer");
    scanf("%d",&i);
    k=i;
    while(k!=0)
    {
        j=k%10;
        rev=rev*10+j;
        k=k/10;
    }
    if(i==rev)
    printf("%d is a palindrome",i);
    else
    printf("%d is not a palindrome",i);

    return 0;
}
